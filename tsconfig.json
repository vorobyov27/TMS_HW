{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "types":["node", "cypress", "cypress-xpath"],
    "esModuleInterop": true,
    "target": "es2016",
    "lib": ["DOM", "ES2016"],
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "skipLibCheck": true
  },
  "ts-node": {"esm": true}
}