{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "node",
    "types":["node", "cypress", "chromedriver", "selenium-webdriver", "jest", "superagent", "playwright-core", "playwright"],
    "esModuleInterop": true,
    "target": "es2016",
    "lib": ["DOM", "ES2016"],
    "forceConsistentCasingInFileNames": true, 
    "strict": true, 
    "skipLibCheck": true
  },
  "ts-node": {"esm": false}
}